<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/test-ontology#"
     xml:base="http://example.org/test-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Ontology rdf:about="http://example.org/test-ontology">
        <rdfs:label>Test Ontology</rdfs:label>
        <rdfs:comment>A small test ontology for unit testing</rdfs:comment>
    </owl:Ontology>

    <!-- Level 1: Direct subclasses of Thing -->

    <owl:Class rdf:about="http://example.org/test-ontology#Energy">
        <rdfs:label>Energy</rdfs:label>
        <rdfs:comment>Represents energy concepts</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/test-ontology#Measurement">
        <rdfs:label>Measurement</rdfs:label>
        <rdfs:comment>Represents measurement concepts</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/test-ontology#Device">
        <rdfs:label>Device</rdfs:label>
        <rdfs:comment>Represents device concepts</rdfs:comment>
    </owl:Class>

    <!-- Level 2: Subclasses of Energy -->

    <owl:Class rdf:about="http://example.org/test-ontology#ElectricalEnergy">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Energy"/>
        <rdfs:label>Electrical Energy</rdfs:label>
        <rdfs:comment>Energy in electrical form</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/test-ontology#ThermalEnergy">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Energy"/>
        <rdfs:label>Thermal Energy</rdfs:label>
        <rdfs:comment>Energy in thermal form</rdfs:comment>
    </owl:Class>

    <!-- Level 2: Subclasses of Measurement -->

    <owl:Class rdf:about="http://example.org/test-ontology#Power">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Measurement"/>
        <rdfs:label>Power</rdfs:label>
        <rdfs:comment>Power measurement</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/test-ontology#Temperature">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Measurement"/>
        <rdfs:label>Temperature</rdfs:label>
        <rdfs:comment>Temperature measurement</rdfs:comment>
    </owl:Class>

    <!-- Level 2: Subclasses of Device -->

    <owl:Class rdf:about="http://example.org/test-ontology#Sensor">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Device"/>
        <rdfs:label>Sensor</rdfs:label>
        <rdfs:comment>A sensing device</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/test-ontology#Meter">
        <rdfs:subClassOf rdf:resource="http://example.org/test-ontology#Device"/>
        <rdfs:label>Meter</rdfs:label>
        <rdfs:comment>A metering device</rdfs:comment>
    </owl:Class>

</rdf:RDF>
